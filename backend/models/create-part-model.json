{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "part",
    "type": "object",
    "properties": {
      "description": {
        "type": "string",
        "minLength": 2,
        "pattern": "^[^-\\s][a-zA-Z0-9_\\s-]+$"
      },
      "vendorPartNum": {
        "type": "string",
        "minLength": 2,
        "pattern": "^[^-\\s][a-zA-Z0-9_\\s-]+$"
      },
      "vendor": {
        "type": "string",
        "minLength": 2,
        "pattern": "^[^-\\s][a-zA-Z0-9_\\s-]+$"
      },
      "inStock": {
        "type": "number",
        "minimum": 0
      }
    },
    "required": [
      "description",
      "vendorPartNum",
      "vendor",
      "inStock"
    ],
    "additionalProperties": false
  }
  